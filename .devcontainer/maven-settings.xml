<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      https://maven.apache.org/xsd/settings-1.0.0.xsd">
	<localRepository>/usr/share/maven/ref/repository</localRepository>
	<mirrors>
		<mirror>
			<id>nexus</id>
			<mirrorOf>central</mirrorOf>
			<url>https://nexus.shawinc.com/repository/public/</url>
		</mirror>
		<mirror>
			<id>nexus-redhat-ga</id>
			<mirrorOf>redhat-ga</mirrorOf>
			<url>https://nexus.shawinc.com/repository/redhat-ga-repository/</url>
		</mirror>

		<mirror>
			<id>nexus-jboss-public-repository-group</id>
			<mirrorOf>jboss-public-repository-group</mirrorOf>
			<url>https://nexus.shawinc.com/repository/JBossPub/</url>
		</mirror>


	</mirrors>
	<profiles>
		<profile>
			<id>nexus</id>
			<repositories>
				<repository>
					<id>central</id>
					<url>http://ignored</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</repository>
				<repository>
					<id>redhat-ga</id>
					<url>http://ignored</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</repository>
				<!--
			<repository>
				<id>jboss-public-repository-group</id>
				<url>http://ignored</url>
				<releases>
					<enabled>true</enabled>
				</releases>
				<snapshots>
					<enabled>false</enabled>
				</snapshots>
			</repository>
			 -->
			</repositories>
			<pluginRepositories>
				<pluginRepository>
					<id>central</id>
					<url>http://central</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</pluginRepository>
				<pluginRepository>
					<id>redhat-ga</id>
					<url>http://central</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</pluginRepository>
			</pluginRepositories>
		</profile>
	</profiles>
	<activeProfiles>

		<activeProfile>nexus</activeProfile>
	</activeProfiles>
</settings>